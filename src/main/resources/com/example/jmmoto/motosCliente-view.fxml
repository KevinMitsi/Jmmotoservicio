<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="338.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jmmoto.controllers.MotosClienteViewController">
    <Label alignment="CENTER" contentDisplay="CENTER" layoutX="251.0" layoutY="14.0" text="Tus motos" textAlignment="CENTER">
        <font>
            <Font name="System Bold" size="20.0" />
        </font>
    </Label>
    <Hyperlink fx:id="hlVolver" layoutX="14.0" layoutY="17.0" onAction="#onVolverLinkClick" text="Volver" underline="true" />
    <TableView fx:id="tableMoto" layoutX="10.0" layoutY="125.0" prefHeight="200.0" prefWidth="580.0">
        <columns>
            <TableColumn fx:id="colPlaca" prefWidth="218.4000244140625" text="PLACA" />
            <TableColumn fx:id="colMarca" prefWidth="178.39996337890625" text="MARCA" />
            <TableColumn fx:id="colModelo" prefWidth="180.0" text="MODELO" />
        </columns>
    </TableView>
    <Button fx:id="btnEliminar" layoutX="10.0" layoutY="86.0" mnemonicParsing="false" onAction="#onEliminarButtonClick" prefHeight="26.0" prefWidth="290.0" text="Eliminar moto" />
    <Button fx:id="btnAgregar" layoutX="300.0" layoutY="86.0" mnemonicParsing="false" onAction="#onAgregarButtonCLick" prefHeight="26.0" prefWidth="290.0" text="Agregar moto" />

</AnchorPane>
